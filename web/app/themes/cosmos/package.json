{
  "name": "cosmos",
  "version": "3.0.0-alpha.1",
  "author": "BigUniverse <hello@biguniverse.ru>",
  "homepage": "https://biguniverse.ru",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/rslnk/biguniverse.git"
  },
  "bugs": {
    "url": "https://github.com/rslnk/biguniverse/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "scripts": {
    "build": "npm run build:templates && npm run build:iconfont && webpack --progress --config assets/build/webpack.config.js",
    "build:production": "npm run build -s -- -p",
    "watch:webpack": "webpack --progress --config assets/build/webpack.config.js",
    "start": "npm run build:templates && npm run build:iconfont && concurrently --kill-others --raw 'npm run watch:webpack -s -- --watch' 'npm run watch:views' 'npm run watch:icons'",
    "clean": "rimraf dist",
    "lint": "eslint assets/scripts assets/build",
    "test": "npm run lint -s",
    "watch:views": "onchange 'assets/views/**/*.jade' -- npm run build:templates",
    "watch:icons": "onchange 'assets/images/icons/**/*.svg' -- npm run build:iconfont",
    "build:templates": "gulp templates",
    "build:iconfont": "gulp iconfont"
  },
  "engines": {
    "node": ">= 4.5"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "async": "^2.1.2",
    "autoprefixer": "^6.4.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "body-parser": "^1.15.2",
    "browser-sync": "^2.15.0",
    "clean-webpack-plugin": "^0.1.10",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "cssnano": "^3.7.4",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-config-airbnb-es5": "^1.0.9",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "glob-all": "^3.1.0",
    "gulp": "^3.9.1",
    "gulp-consolidate": "^0.2.0",
    "gulp-iconfont": "^8.0.1",
    "gulp-jade-php": "^2.0.0-0",
    "gulp-plumber": "^1.1.0",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-webpack-plugin": "^1.1.0",
    "imports-loader": "^0.6.5",
    "jeet": "^6.1.5",
    "kouto-swiss": "^0.13.1",
    "lodash": "^4.15.0",
    "minimist": "^1.2.0",
    "monkey-hot-loader": "github:rmarscher/monkey-hot-loader#webpack2-import",
    "onchange": "^3.0.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "plumber": "^0.4.8",
    "postcss": "^5.2.0",
    "postcss-loader": "^0.13.0",
    "qs": "^6.2.1",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.4",
    "rupture": "^0.6.2",
    "style-loader": "^0.13.1",
    "stylus-loader": "^2.3.1",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-middleware": "^1.7.0",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "jquery": "1.12.4 - 3"
  }
