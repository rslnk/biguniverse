- get_template_part('partials/page-header')
- if (!have_posts()) :
  - _e('Sorry, no results were found.', 'cosmos')
  - get_search_form()
- endif

- while (have_posts()) :
  - the_post();
  - get_template_part('content/preview', get_post_type() != 'post' ? get_post_type() : get_post_format())
- endwhile

- the_posts_navigation()
