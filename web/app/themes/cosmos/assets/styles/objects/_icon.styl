/**
 * Iconfont icon (object)
 *
 *    Generated with `gulp-iconfont` task using template at `styles/templates/_icon.styl`
 *    @link: https://www.npmjs.com/package/gulp-iconfont
 */
@font-face
  font-family 'CosmosIcons'
  src url('../fonts/CosmosIcons.eot')
  src url('../fonts/CosmosIcons.eot?#iefix') format('eot'),
    url('../fonts/CosmosIcons.woff') format('woff'),
    url('../fonts/CosmosIcons.ttf') format('truetype'),
    url('../fonts/CosmosIcons.svg#CosmosIcons') format('svg')
  font-weight normal
  font-style normal

.o-icon
  display block
  speak none
  font-family 'CosmosIcons'
  font-weight normal
  font-style normal
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale